<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:core="sap.ui.core"

    
>
    <List
        id="_IDGenList1"
        headerText="{i18n>UnitsPlannerMaster.title}"
        mode="None"
        items="{path: '/FloorUnits'}"        
    >

    	<headerToolbar>
			<OverflowToolbar>
				<Title text="Products" level="H2" />
				<ToolbarSpacer />
				<MultiComboBox id="idSticky"
					placeholder="Sticky options"
					selectionFinish="onSelectionFinish"
					width="15%">
					<items>
						<core:Item  text="Header Toolbar"
							key="HeaderToolbar"/>
						<core:Item  text="Info Toolbar"
							key="InfoToolbar"/>
					</items>
				</MultiComboBox>
				<ToggleButton id="toggleInfoToolbar"
					text="Hide/Show InfoToolbar"
					press="onToggleInfoToolbar" />
				<Button
					icon="sap-icon://settings"
					press="handleButtonPress" />
				<Button
					icon="sap-icon://person-placeholder"
					press="handleButtonPress" />
				<Button
					icon="sap-icon://drop-down-list"
					press="handleButtonPress" />
			</OverflowToolbar>
		</headerToolbar>
        
        <CustomListItem id="_IDGenCustomListItem1">
            <HBox id="_IDGenHBox1">
                <Label
                    id="_IDGenLabel3"
                    text="fl"
                    class="sapUiSmallMarginTopBottom sapUiSmallMarginBegin"
                />
                <Label
                    id="_IDGenLabel4"
                    text="{Floor}"
                    class="floorFont sapUiSmallMarginTopBottom sapUiTinyMarginBegin"
                />

                <HBox
                    id="_IDGenHBox2"
                    alignItems="Center"
                    justifyContent="SpaceBetween"
                    width="100%"
                >
                    <VBox
                        id="_IDGenVBox2"
                        class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom"
                    >
                        <Title
                            text="{Tenant}"
                            wrapping="true"
                            titleStyle="H3"
                            class=""
                        />

                        <GenericTag
                            text="{Units}"
                            design="Full"
                            status="Success"
                            class="sapUiSmallMarginBottom"
                            press="onListItemPress"
                        >
                            <ObjectNumber
                                emphasized="false"
                                number="{NoOfUnits}"
                                unit="Unit(s)"
                            />
                        </GenericTag>

                        <Label
                            id="_IDGenLabel1"
                            text="{Company}"
                        />
                    </VBox>
                    <VBox
                        class="sapUiSmallMarginEnd"
                        alignContent="Start"
                    >
                        <Title
                            text="Occupation:"
                            wrapping="true"
                            titleStyle=""
                            class=""
                        />
                        <Label
                            text="From: {StartDate}"
                            wrapping="true"
                        />
                        <Label
                            text="To: {LastDate}"
                            wrapping="true"
                        />
                        <ObjectNumber
                            emphasized="false"
                            number="{NoOfContract}"
                            unit="Staggered(s)"
                        />
                    </VBox>
                </HBox>
                <!-- <HBox
                    id="_IDGenHBox3"
                    alignItems="Center"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <core:Icon
                        id="_IDGenIcon1"
                        src="sap-icon://navigation-right-arrow"
                        class="size3"
                        color="#E69A17"
                    />
                </HBox> -->
            </HBox>
        </CustomListItem>
    </List>
</core:FragmentDefinition>
