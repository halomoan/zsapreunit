<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
    
>
<Page showHeader="false" >

		<List
			id="sfloorunit"		
			items="{/SFloorUnitSet}"
			sticky="HeaderToolbar"            
		>

        <headerToolbar>
			<OverflowToolbar>
				<Title text="{i18n>UnitsPlannerMaster.title}" level="H2" />
				<ToolbarSpacer />				
				<SearchField liveChange=".onListSearch" width="25%"/>
			</OverflowToolbar>
		</headerToolbar>

			<CustomListItem id="_IDGenCustomListItem1">
				<HBox id="_IDGenHBox1">
					<Label
						id="_IDGenLabel3"
						text="fl"
						class="sapUiSmallMarginTopBottom sapUiSmallMarginBegin"
					/>
					<Label
						id="_IDGenLabel4"
						text="{Floor}"
						class="floorFont sapUiSmallMarginTopBottom sapUiTinyMarginBegin"
					/>
					<HBox
						id="_IDGenHBox2"
						alignItems="Center"
						justifyContent="SpaceBetween"
						width="100%"
					>
						<VBox
							id="_IDGenVBox2"
							class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom"
						>
							<Title
								text="{Company}"
								wrapping="true"
								titleStyle="H3"
								class=""
							/>
							<GenericTag
								text="{Unitnos}"
								design="StatusIconHidden"
								status="Information"
								class="sapUiSmallMarginBottom"
								press="onListItemPress"
							>
								<ObjectNumber
									emphasized="false"
									number="{Noofunit}"
									unit="Unit(s)"
								/>
							</GenericTag>
							<Label
								id="_IDGenLabel1"
								text="{Tenant}"
							/>
						</VBox>
						<VBox
							class="sapUiSmallMarginEnd"
							alignContent="Start"
						>
							<Title
								text="Occupation:"
								wrapping="true"
								titleStyle="Auto"
								class=""
							/>
							<Label
								text="From: {
									path: 'Startdate',
									formatter: '.formatDateTime'
								}"
								wrapping="true"
							/>
							<Label
								text="To: {
									path: 'Lastdate',
									formatter: '.formatDateTime'
								}"
								wrapping="true"
							/>
							<ObjectNumber
								emphasized="false"
								number="{Noofcontr}"
								unit="Contract(s)"
							/>
						</VBox>
					</HBox>
					<!-- <HBox
                    id="_IDGenHBox3"
                    alignItems="Center"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <core:Icon
                        id="_IDGenIcon1"
                        src="sap-icon://navigation-right-arrow"
                        class="size3"
                        color="#E69A17"
                    />
                </HBox> -->
				</HBox>
			</CustomListItem>
		</List>	
</Page>
</core:FragmentDefinition>