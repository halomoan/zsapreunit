<mvc:View
	controllerName="zsapreunit.pages.controller.FloorUnitPlanner"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.uxap"
	xmlns:t="sap.ui.table"
	xmlns:m="sap.m"
	xmlns:plugins="sap.ui.table.plugins"
	xmlns:core="sap.ui.core"
	xmlns:l="sap.ui.layout"
	height="100%"
	class="sapUiSizeCompact"
>
	<m:Page>
		<m:headerContent>
			<m:Button
				text="{i18n>Label.UploadData}"
				tooltip="Upload data to server"
				visible="{viewData>/isDirty}"
				press="onUploadToServer"
			/>
			<m:ToolbarSpacer/>
			<m:Button
				text="{i18n>Label.SecondTerm}"
				tooltip="2nd Term"
				visible="{viewData>/showBtn2ndTerm}"
				press="onCreate2ndTermDialog"
			/>
			<m:Button
				text="{i18n>Label.DeleteSecondTerm}"
				tooltip="2nd Term"
				visible="{viewData>/showBtnDel2ndTerm}"
				press="onDelete2ndTermDialog"
			/>
			<m:Button
				text="{i18n>Label.ThirdTerm}"
				tooltip="2nd Term"
				visible="{viewData>/showBtn3rdTerm}"
				press="onCreate3rdTermDialog"
			/>
			<m:Button
				text="{i18n>Label.DeleteThirdTerm}"
				tooltip="2nd Term"
				visible="{viewData>/showBtnDel3rdTerm}"
				press="onDelete3rdTermDialog"
			/>
		</m:headerContent>
		<t:Table
			id="planTable"
			rows="{
				path: 'tableData>/floorData',
				templateShareable: false
			}"
			ariaLabelledBy="title"
			enableColumnFreeze="true"
			visibleRowCount="{viewData>/noRows}"
			enableBusyIndicator="true"
		>
			<t:plugins>
				<plugins:MultiSelectionPlugin
					limit="20"
					enableNotification="true"
					showHeaderSelector="true"
					selectionMode="MultiToggle"
					selectionChange=".onUnitSelChange"
				/>
			</t:plugins>
			<t:extension>
				<m:OverflowToolbar style="Clear">
					<m:ToolbarSpacer/>
					<m:Button
						icon="sap-icon://close-command-field"
						textDirection="Inherit"
						type="Default"
						press="onGotoFirstColumn"
					/>
				</m:OverflowToolbar>
			</t:extension>
			<t:columns>
				<t:Column
					width="4rem"
					filterProperty="Floor"
				>
					<m:Label text="{i18n>Label.Floor}"/>
					<t:template>
						<m:Label
							text="{tableData>Floor}"
							design="{= ${tableData>Term1mode/Main} ? 'Bold': 'Standard'}"
							width="100%"
						/>
					</t:template>
				</t:Column>
				<t:Column
					width="4rem"
					filterProperty="Unitno"
				>
					<m:Label text="{i18n>Label.UnitNo}"/>
					<t:template>
						<m:Label
							text="{tableData>Unitno}"
							design="{= ${tableData>Term1mode/Main} ? 'Bold': 'Standard'}"
							textAlign="{= ${tableData>Term1mode/Main} ? 'Begin': 'End'}"
							width="100%"
						/>
					</t:template>
				</t:Column>
				<t:Column
					width="18rem"
					headerSpan="2"
					filterProperty="Term1/Company"
				>
					<t:multiLabels>
						<m:Label
							text="First Term"
							textAlign="Center"
							width="100%"
						/>
						<m:Label
							text="{i18n>Contract.Company}"
							textAlign="Center"
							width="100%"
						/>
					</t:multiLabels>
					<t:template>
						<m:Label
							text="{tableData>Term1/Company}"
							design="{= ${tableData>Term1mode/Main} ? 'Bold': 'Standard'}"
						/>
					</t:template>
				</t:Column>
				<t:Column
					width="12rem"
					filterProperty="Term1/Tenant"
				>
					<t:multiLabels>
						<m:Label text="First Term"/>
						<m:Label
							text="{i18n>Contract.Tenant}"
							textAlign="Center"
							width="100%"
						/>
					</t:multiLabels>
					<t:template>
						<m:Label
							text="{tableData>Term1/Tenant}"
							design="{= ${tableData>Term1mode/Main} ? 'Bold': 'Standard'}"
						/>
					</t:template>
				</t:Column>
				<t:Column width="10rem">
					<t:multiLabels>
						<m:Label
							text="First Term"
							textAlign="Center"
							width="100%"
						/>
						<m:Label
							text="Trade"
							textAlign="Center"
							width="100%"
						/>
					</t:multiLabels>
					<t:template>
						<m:Label
							text="{tableData>Term1/Trade}"
							design="{= ${tableData>Term1mode/Main} ? 'Bold': 'Standard'}"
						/>
					</t:template>
				</t:Column>
				<t:Column width="6rem">
					<t:multiLabels>
						<m:Label
							text="First Term"
							textAlign="Center"
							width="100%"
						/>
						<m:Label
							text="{i18n>Contract.AreaSize}"
							textAlign="Center"
							width="100%"
						/>
					</t:multiLabels>
					<t:template>
						<!-- <m:Label
							text="{
								path: 'tableData>Term1/Areasize',
								formatter: '.formatNoDecimals'
							} {tableData>Term1/Uom}"
							design="{= ${tableData>Term1mode/Main} ? 'Bold': 'Standard'}"
							visible="{= ${tableData>Term1mode/Main}}"
							textAlign="End"
							width="100%"
						/> -->
						<m:ObjectNumber
							textAlign="End"
							number="{
								path: 'tableData>Term1/Areasize',
								formatter: '.formatNoDecimals'
							}"
							unit="{tableData>Term1/Uom}"
						/>
					</t:template>
				</t:Column>
				<t:Column
					width="5rem"
					hAlign="Center"
				>
